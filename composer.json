{
    "name": "fitpass/passwordgenerator",
    "description": "Password Generator for Laravel with length and strength conditions.",
    "type": "laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Milenko Letic",
            "email": "letic.milenko1993@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "FitPass\\PasswordGenerator\\": "src/",
            "FitPass\\PasswordGenerator\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "FitPass\\PasswordGenerator\\PasswordGeneratorServiceProvider"
            ],
            "aliases": {
                "PasswordGenerator": "FitPass\\PasswordGenerator\\Facades"
            }
        }
    },
    "require": {
        "php": "^8.1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.10",
        "orchestra/testbench": "8.0.x-dev"
    }
}
